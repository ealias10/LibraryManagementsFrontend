{
  "name": "DASHBOARD-UI",
  "description": "Dashboard frontend",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "proxy": "http://localhost:7000",
  "keywords": [
    "SHA",
    "web",
    "portal"
  ],
  "scripts": {
    "dev": "env-cmd -f .env/.env.dev next",
    "build:dev": "env-cmd -f .env/.env.dev next build",
    "build:staging": "env-cmd -f .env/.env.staging next build",
    "build:prod": "env-cmd -f .env/.env.prod next build",
    "local": "env-cmd -f ./.env/.local.dev next dev",
    "start": "next start",
    "lint": "eslint  --ignore-path .gitignore \"src/**/*.+(js|jsx)\" ",
    "lint-fix": "eslint  --ignore-path .gitignore \"src/**/*.+(js|jsx)\" --fix",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(js|jsx)\" --write"
  },
  "dependencies": {
    "@emotion/react": "11.8.1",
    "@emotion/styled": "11.8.1",
