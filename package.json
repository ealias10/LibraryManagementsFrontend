    "@mui/icons-material": "5.4.2",
    "@mui/material": "5.5.0",
    "@react-pdf/renderer": "2.0.21",
    "antd": "^5.7.0",
    "async": "3.2.3",
    "axios": "0.21.1",
    "babel-eslint": "10.1.0",
    "date-fns": "2.28.0",
    "file-saver": "2.0.5",
